{
    "name": "concept-labs/config",
    "type": "library",
    "description": "(C)oncept-Labs Config Manager",
    "keywords": ["php","config"],
    "license": "MIT",
    "authors": [
        {
            "name": "Viktor Halytskyi",
            "email": "concept.galitsky@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "concept-labs/arrays": "^1"
    },
    "autoload": {
        "psr-4": {
            "Concept\\Config\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0"
    },
    "scripts": {
        "test": "pest",
        "test:unit": "pest tests/Unit",
        "test:feature": "pest tests/Feature",
        "test:coverage": "pest --coverage"
    },
    "autoload-dev": {
        "psr-4": {
            "Concept\\Config\\Test\\": "test/"
        }
    },
    "extra": {
        "concept": {
            "include": "concept.json"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
